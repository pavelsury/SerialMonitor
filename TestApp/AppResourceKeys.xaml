<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="CheckBoxStyleKey" TargetType="{x:Type CheckBox}" BasedOn="{StaticResource {x:Type CheckBox}}"/>
    <Style x:Key="ComboBoxStyleKey" TargetType="{x:Type ComboBox}" BasedOn="{StaticResource {x:Type ComboBox}}"/>
    <Style x:Key="ButtonStyleKey" TargetType="{x:Type Button}" BasedOn="{StaticResource {x:Type Button}}"/>
    <Style x:Key="TextBoxStyleKey" TargetType="{x:Type TextBox}" BasedOn="{StaticResource {x:Type TextBox}}"/>
    <Style x:Key="ThemedDialogLabelStyleKey" TargetType="{x:Type Label}" BasedOn="{StaticResource {x:Type Label}}"/>
    <Style x:Key="ThemedDialogHyperlinkStyleKey" TargetType="{x:Type Hyperlink}" BasedOn="{StaticResource {x:Type Hyperlink}}"/>
    <Brush x:Key="WindowPanelTextBrushKey">Black</Brush>
    <Brush x:Key="ButtonBorderBrushKey">LightGray</Brush>
    <Brush x:Key="SelectedItemActiveBrushKey">LightSkyBlue</Brush>
    <Brush x:Key="SelectedItemActiveTextBrushKey">Black</Brush>
    <Brush x:Key="ToolWindowTextBrushKey">Black</Brush>
    <Brush x:Key="TextBoxTextBrushKey">Black</Brush>
</ResourceDictionary>
